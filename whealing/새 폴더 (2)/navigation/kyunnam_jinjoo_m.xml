<?xml version="1.0" encoding="utf-8"?>
<!--충남 공주-->
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:padding="30dp"
        android:id="@+id/kyungnam_jinjoo_not_free_pop"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/textView_"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="10dp"
            android:text="자전거 이용방법" />
        <TextView
            android:id="@+id/textView_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="10dp"
            android:text="운영시간 : 하절기 : 08:00~20:00/동절기 : 09:00~18:00\n대여료 : 일반/여성/어린이용 4000원, 2인용 8000원" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="10dp"
            android:text="관련페이지 url" />

        <TextView
            android:id="@+id/textView_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="10dp"
            android:linksClickable="true"
            android:autoLink="web"
            android:text="http://www.jinju.go.kr/00135/01110/01171.web"/>

    </LinearLayout>
</ScrollView>